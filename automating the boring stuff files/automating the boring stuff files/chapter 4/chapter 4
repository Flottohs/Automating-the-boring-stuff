print('hi,guy,si',sep=',', end=' ')

#with the print function, it has 2 parameters including sep(separating character) and end(ending character/, automatic ending character is a enter going to the next line)

#call stack

def a():
    print()
    print('a() starts')
    b()
    d()
    print('a() returns')

def b():
    print('b() starts')
    c()
    print('b() returns')

def c():
    print('c() starts')
    print('c() returns')

def d():
    print('d() starts')
    print('d() returns')

a()