import random
flips = []
number_of_streaks = 0


for _ in range(100):
    flips.append('H' if random.randint(0,1) == 1 else 'T')
    
    

for start in range(len(flips)-5):
    slice = flips[start:start+6]
    if all(slice[i] == slice[0] for i in range(6)):
        number_of_streaks +=1
       


print(list)
print('Chance of streak: %s%%' % (number_of_streaks / 100))
